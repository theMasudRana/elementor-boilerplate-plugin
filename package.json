  },
  "scripts": {
    "prepare": "husky install",
    "test-old": "eslint --quiet . && stylelint --syntax scss -q **/*.scss && composer run-script phpcs",
    "test": "eslint --quiet .",
    "test-js": "eslint --quiet .",
    "test-scss": "stylelint --syntax scss -q **/*.scss",
    "test-php": "composer run-script phpcs",
    "style-fix": "stylelint --syntax scss **/*.scss --fix",
    "js-fix": "eslint . --fix",
    "build": "NODE_ENV=production gulp build",
    "watch": "NODE_ENV=development gulp watch",
